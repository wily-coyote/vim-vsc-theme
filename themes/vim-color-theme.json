	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment",
			],
			"settings": {
				"foreground": "#80a0ff"
			}
		},
		{
			"name": "Reset",
			"scope": [
				"keyword.operator.assignment",
				"meta.use.rust punctuation",
				"storage.modifier.array"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword",
				"keyword.control",
				"keyword.operator.new",
				"keyword.other",
				"keyword.operator.c",
				"punctuation.brackets.angle.rust",
				"support.type.property-name.json",
				"keyword.other storage.type.rust"
			],
			"settings": {
				"foreground": "#ffff60"
			}
		},
		{
			"name": "Type",
			"scope": [
				"meta.type",
				"support.type",
				"storage.type",
				"entity.name.type",
				"keyword.other.typedef",
				"storage.modifier",
				"entity.name.section"
			],
			"settings": {
				"foreground": "#60ff60"
			}
		},
		{
			"name": "Constant",
			"scope": [
				"keyword.other.unit",
				"constant.numeric",
				"support.constant",
				"constant.other",
				"constant.numeric.float",
				"constant.numeric.hex",
				"constant.numeric.integer",
				"constant.numeric.octal",
				"constant.language",
				"entity.name.type.option.rust",
				"string",
				"punctuation.definition.interpolation.rust"
			],
			"settings": {
				"foreground": "#ffa0a0"
			}
		},
		{
			"name": "Special",
			"scope": [
				"constant.character.escape",
				"constant.other.placeholder",
				"keyword.operator.namespace"
			],
			"settings": {
				"foreground": "#ffa500"
			}
		},
		{
			"name": "Function",
			"scope": [
				"support.function",
				"entity.name.function",
				"variable.parameter",
				"meta.interpolation"
			],
			"settings": {
				"foreground": "#40ffff"
			}
		},
		{
			"name": "Preproc",
			"scope": [
				"meta.preprocessor keyword.control",
				"entity.name.function.preprocessor",
				"meta.preprocessor",
				"constant.language.c",
				"meta.attribute",
				"meta.use",
				"meta.macro",
				"meta.macro entity.name.function",
				"keyword.control.directive"
			],
			"settings": {
				"foreground": "#ff80ff"
			}
		}
	]
}
